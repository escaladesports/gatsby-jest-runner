{
  "name": "gatsby-jest-runner",
  "version": "0.0.0",
  "keywords": [
    "gatsby",
    "jest",
    "runner"
  ],
  "main": "dist/index.js",
  "author": "Matt Suhay <msuhay@escaladesports.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/escaladesports/gatsby-jest-runner.git"
  },
  "license": "MIT",
  "scripts": {
    "dev": "gatsby develop --open",
    "build": "rm -rf dist && mkdir dist && babel src/module --out-dir dist",
    "build:gatsby": "gatsby build",
    "lint": "eslint ./src",
    "precommit": "lint-staged",
    "reset": "node utils/reset",
    "test": "jest",
    "travis": "opn https://travis-ci.org/escaladesports/gatsby-jest-runner/branches"
  },
  "jest": {
    "testRegex": "/.*(__tests__\\/.*)|(.*(test|spec))\\.jsx?$",
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss)$": "identity-obj-proxy",
      ".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/setupTests.js",
    "testPathIgnorePatterns": [
      "node_modules",
      ".cache",
      "__snapshots__",
      "public",
      "dist"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(gatsby)/)"
    ],
    "globals": {
      "__PATH_PREFIX__": ""
    },
    "testURL": "http://localhost",
    "setupFiles": [
      "<rootDir>/loadershim.js"
    ]
  },
  "babel": {
    "env": {
      "test": {
        "presets": [
          "es2015",
          "react",
          "stage-0"
        ]
      }
    }
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-emotion": "^9.2.6",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "child-process-promise": "^2.2.1",
    "emotion": "^9.2.6",
    "emotion-server": "^9.2.6",
    "enzyme": "^3.4.1",
    "enzyme-adapter-react-16": "^1.2.0",
    "eslint": "^5.3.0",
    "eslint-config-escalade-sports": "^1.0.1",
    "fs-extra": "^7.0.0",
    "gatsby": "^1.9.277",
    "gatsby-plugin-emotion": "^1.1.17",
    "gatsby-plugin-html-attributes": "^1.0.4",
    "gatsby-plugin-react-helmet": "^2.0.11",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.4.2",
    "jest-dom": "^1.12.0",
    "lint-staged": "^7.2.0",
    "opn-cli": "^3.1.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-helmet": "^5.2.0",
    "react-test-renderer": "^16.4.2",
    "react-testing-library": "^4.1.7"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0"
  }
}
